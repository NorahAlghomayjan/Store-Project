{
  "name": "creating-api-with-postgres",
  "version": "1.0.0",
  "description": "## Getting Started",
  "main": "build/server.js",
  "scripts": {
    "test": "ENV=test && npx tsc && db-migrate --env test up && jasmine && db-migrate db:drop test",
    "posttest": "db-migrate reset --env test",
    "build": "npx tsc",
    "jasmine": "jasmine",
    "prestart": "ENV=dev",
    "start": "ENV=dev && tsc && node build/server.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts db-migrate reset --env test",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./build --onSuccess 'node ./build/server.js'",
    "tsc": "tsc",
    "create-migration": "db-migrate create --sql-file --",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/prettier": "^2.7.1",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "pg": "^8.8.0",
    "tsconfig-paths": "^4.1.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.14",
    "@types/jasmine": "^4.3.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine": "^4.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.0",
    "supertest": "^6.3.1",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.9.3"
  }
}
